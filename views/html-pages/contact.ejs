<%- include ('../partials/main-nav') %> 

    <main class="main-contact main-signup">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="#0099ff" fill-opacity="1" d="M0,224L48,240C96,256,192,288,288,293.3C384,299,480,277,576,245.3C672,213,768,171,864,170.7C960,171,1056,213,1152,240C1248,267,1344,277,1392,282.7L1440,288L1440,0L1392,0C1344,0,1248,0,1152,0C1056,0,960,0,864,0C768,0,672,0,576,0C480,0,384,0,288,0C192,0,96,0,48,0L0,0Z"></path></svg>
        <div class="container">
            <section class="main-sec">
                <div class="form">
                    <form action="#" >
                        <div class="content">
                            <label for="name">full name</label>
                            <input type="text" name="name" required>
                        </div>
                        <div class="content">
                            <label for="email">email address</label>
                            <input type="email" name="email" required>
                        </div>
                        <div class="content">
                            <label for="phone">phone</label>
                            <input type="number" name="phone" required>
                        </div>
                        <div class="content">
                            <label for="subject">subject</label>
                            <input type="text" name="subject" required>
                        </div>

                        <div class="textarea">
                            <textarea name="contact-body" cols="20" rows="10" required></textarea>
                        </div>

                        <div class="contact-btn">
                            <button type="submit">submit</button>
                        </div>
                    </form>
                </div>
            </section>
        </div>
    </main>

